{
  "name": "MLB.com Prospect Watch Visualizer",
  "description": "Visualize position info of MLB.com Prospect Watch.",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["http://m.mlb.com/prospects/*"],
    "js": ["jquery-2.2.0.min.js", "content.js"]
  }],
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
